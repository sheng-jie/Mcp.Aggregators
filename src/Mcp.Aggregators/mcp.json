{
    "mcpServers": {
        "fetch": {
            "command": "uvx",
            "args": [
                "mcp-server-fetch"
            ],
            "env": {
                "port": 3300,
                "node-env": "dev"
            }
        }, 
        "samplemcpserver": {
            "type": "stdio",
            "command": "dnx",
            "args": [
                "sheng-jie.SampleMcpServer","--yes"
            ],
            "env": {
                "WEATHER_CHOICES": "sunny,humid,freezing,perfect"
            }
        },
        "time": {
            "type": "sse",
            "url": "https://mcp.api-inference.modelscope.net/0506441bba8744/sse"
        }
    }
}